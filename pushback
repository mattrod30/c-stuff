#include<iostream>
#include<string>
#include<vector>
using namespace std;

void printArray(const int* a, int size) {
    cout << "[ ";
    for (int i = 0; i < size; i++) {
        cout << a[i] << " ";
    }
    cout << "]\n";
}
template<typename T>
T* pushBack(const T* a, int size, T newVal);


