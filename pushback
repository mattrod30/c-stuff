template<typename T>
T* pushBack(const T* a, int size, T newVal) {


    T* pushBack(const T * a, int size) {

    T* b = new T[size+1];

    for (int i = 0; i < size; i++) {
        b[i] = a[i];
    }
    b[size] = newVal;

    return b;
}
